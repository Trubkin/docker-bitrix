# Project name to separate multiple instances
COMPOSE_PROJECT_NAME=sotbit

# PHP version settings.
PHP_VERSION=php

# Web server type.
WEB_SERVER_TYPE=nginx

# MySQL settings.
DB_SERVER_TYPE=mysql
MYSQL_DATABASE=bitrix
MYSQL_USER=bitrix
MYSQL_PASSWORD=123
MYSQL_ROOT_PASSWORD=123

# Network
INTERFACE=127.0.0.1

# Site path
SITE_PATH=/sites
DOCUMENT_ROOT=/var/www/